<br><br>

<div class="container">
  <div class="card card-custom gutter-b">
      <div class="row">
         <div class="col-lg-12">
            <div class="card-header">
              <div class="card-title">
                <h3 class="card-label float-left ">Entrega de vehículo</h3>
                <%= link_to 'Regresar', vehicle_checklists_path, class:"btn btn-default float-right" %>
                </div>
             </div>
         </div>
      </div>
    <div class="card-body">
      <p>
        <strong>Clasificación:</strong>
        <%= @vehicle_checklist.clasificacionvehiculo %>
      </p>

      <p>
        <strong>Concepto:</strong>
        <%= @vehicle_checklist.conceptovehiculo %>
      </p>

      <p>
        <strong>Tipo de vehículo:</strong>
        <%= @vehicle_checklist.vehicle_type.descripcion%>
      </p>
      <%= link_to 'Editar', edit_vehicle_checklist_path(@vehicle_checklist),class:"btn btn-default"%>
    </div>
  </div>
</div>
