<br><br>
<div class="container">
  <div class="card card-custom gutter-b">
      <div class="row">
         <div class="col-lg-12">
            <div class="card-header">
              <div class="card-title">
                <h3 class="card-label float-left ">Precio de vehículo</h3>
                  <%= link_to 'Regresar', vehicle_prices_path, class:"btn btn-default float-right" %>
                </div>
             </div>
         </div>
      </div>
    <div class="card-body">

      <p>
        <strong>Línea:</strong>
        <%= @vehicle_price.catalog_brand.descripcion %>
      </p>

      <p>
        <strong>Monto:</strong>
        <%= @vehicle_price.monto %>
      </p>

      <p>
        <strong>Status:</strong>
        <%= @vehicle_price.get_status %>
      </p>
    <%= link_to 'Editar', edit_vehicle_price_path(@vehicle_price), class:"btn btn-default" %>
  </div>
</div>
</div>
