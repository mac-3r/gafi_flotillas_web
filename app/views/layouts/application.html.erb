	<script src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js"></script>
	<!--end::Page Scripts-->

	<div class="modal fade modal_general" tabindex="-1" role="dialog" aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
		<div class="modal-dialog modal-lg">
			
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="titulo_modal_general"></h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						&times;
					</button>
				</div>
				<div class="modal-body" id="contenido">

				</div>
			  </div>
		</div>
	</div>

	<div class="modal fade modal_general_xl" tabindex="-1" role="dialog" aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
