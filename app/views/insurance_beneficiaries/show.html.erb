<br><br>
<div class="container">
  <div class="card card-custom gutter-b">
      <div class="row">
         <div class="col-lg-12">
            <div class="card-header">
              <div class="card-title">
                <h3 class="card-label float-left ">Beneficiario preferente</h3>
                  <%= link_to 'Regresar', insurance_beneficiaries_path, class:"btn btn-default float-right" %>
                </div>
             </div>
         </div>
      </div>
      <div class="card-body">
        <p>
          <strong>Clave:</strong>
          <%= @insurance_beneficiary.clave %>
        </p>
        
        <p>
          <strong>Descripcion:</strong>
          <%= @insurance_beneficiary.descripcion %>
        </p>
        
        <p>
          <strong>Status:</strong>
          <%= @insurance_beneficiary.get_status %>
        </p>
        <%= link_to 'Editar', edit_insurance_beneficiary_path(@insurance_beneficiary), class:"btn btn-default" %>
    </div>
  </div>
</div>
