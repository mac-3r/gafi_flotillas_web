
<%# byebug %>
$("#table_container").append("<%= escape_javascript(render partial: 'tabla') %>");
<% if @vehicles.current_page == @vehicles.total_pages %>
    $('#view-more').remove();
<% else %>
    $('#view-more').append("<%= escape_javascript(link_to('View More', url_for(page: @vehicles.current_page + 1))) %>");
<% end %>