<br>
<div class="container">
  <div class="card card-custom gutter-b">
      <div class="row">
         <div class="col-lg-12">
            <div class="card-header">
              <div class="card-title">
                <h3 class="card-label float-left ">Empresa facturable</h3>
                  <%= link_to 'Regresar',  billed_companies_path, class:"btn btn-default float-right" %>
                </div>
             </div>
         </div>
      </div>
       <div class="card-body">
        <p>
          <strong>Clave jd:</strong>
          <%= @billed_company.clave_jd %>
        </p>

        <p>
          <strong>Nombre:</strong>
          <%= @billed_company.nombre %>
        </p>

        <p>
          <strong>Status:</strong>
          <%= @billed_company.get_status %>
        </p>
        <%= link_to 'Editar', edit_billed_company_path(@billed_company), class:"btn btn-default" %>
       </div>
  </div>
</div>