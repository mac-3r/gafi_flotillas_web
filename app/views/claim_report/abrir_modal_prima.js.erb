<% if @bandera_error %>
    toastr.options = {
        "closeButton": false,
        "debug": false,
        "newestOnTop": false,
        "progressBar": false,
        "positionClass": "toast-top-right",
        "preventDuplicates": false,
        "onclick": null,
        "showDuration": "300",
        "hideDuration": "1000",
        "timeOut": "5000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
    };
    toastr.info("Selecciona un ciclo v√°lido");
<% else %>
    $("#contenido").html("<%= escape_javascript(render partial: 'registrar_prima_form') %>");
    $("#titulo_modal_general").text("Guardar prima anual <%= @anio %> - <%= @anio + 1 %>");
    $(".modal_general").modal("show");
<% end %>