<br><br>
<div class="container">
  <div class="card card-custom gutter-b">
      <div class="row">
         <div class="col-lg-12">
            <div class="card-header">
              <div class="card-title">
                <h3 class="card-label float-left ">Concepto de sucursal</h3>
                  <%= link_to 'Regresar', catalog_branches_path , class:"btn btn-default float-right" %>
                </div>
             </div>
         </div>
      </div>
       <div class="card-body">
          <p>
            <strong>Clave:</strong>
            <%= @catalog_branch.clave %>
          </p>

          <p>
            <strong>Clave jd:</strong>
            <%= @catalog_branch.clave_jd %>
          </p>

          <p>
            <strong>Decripcion:</strong>
            <%= @catalog_branch.decripcion %>
          </p>

          <p>
            <strong>Catalog company:</strong>
            <%= @catalog_branch.catalog_company.nombre %>
          </p>
          <p>
            <strong>Unidad de negocios:</strong>
            <%= @catalog_branch.unidad_negocio %>
          </p>
          <p>
            <strong>Estatus:</strong>
            <%= @catalog_branch.get_status %>
          </p>

          <%= link_to 'Editar', edit_catalog_branch_path(@catalog_branch) %> 
        <br><br>                  
    </div>
  </div>
</div>
