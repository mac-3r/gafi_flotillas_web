<% if @vehiculo %>
    $("#insurance_report_ticket_cedis").val("<%= @vehiculo.catalog_branch.decripcion %>");
    $("#insurance_report_ticket_numero_serie").val("<%= @vehiculo.numero_serie %>");
    $("#insurance_report_ticket_vehiculo").val("<%= "#{@vehiculo.catalog_brand.descripcion}" %>");
    $("#insurance_report_ticket_modelo").val("<%= @vehiculo.catalog_model.descripcion %>");
    $("#insurance_report_ticket_estatus_vehiculo").val("<%= @vehiculo.vehicle_status.descripcion %>");
    $("#insurance_report_ticket_ubicacion_siniestro").val("<%#= @vehiculo.type_sinister.descripcion %>");
    $("#insurance_report_ticket_responsable").val("<%= @vehiculo.catalog_personal.nombre %>");
<% else %>
    $("#insurance_report_ticket_cedis").val("");
    $("#insurance_report_ticket_numero_serie").val("");
    $("#insurance_report_ticket_vehiculo").val("");
    $("#insurance_report_ticket_modelo").val("");
    $("#insurance_report_ticket_estatus_vehiculo").val("");
    $("#insurance_report_ticket_ubicacion_siniestro").val("");
    $("#insurance_report_ticket_responsable").val("");
<% end %>