<% if @bandera_error %>
    toastr.options = {
        "closeButton": false,
        "debug": false,
        "newestOnTop": false,
        "progressBar": false,
        "positionClass": "toast-top-right",
        "preventDuplicates": false,
        "onclick": null,
        "showDuration": "300",
        "hideDuration": "1000",
        "timeOut": "5000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
    };
    toastr.info("<%= @mensaje %>");
<% else %>
    var arr_nombres = [];
    <% @grafica.each do |x| %>
        arr_nombres.push("<%= x[0].cedis %>");
    <% end %>
    $("#table_container").html("<%= escape_javascript(render partial: 'grafica_cedis') %>");
<% end %>