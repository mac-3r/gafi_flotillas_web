<br><br>
<div class="container">
  <div class="card card-custom gutter-b">
      <div class="row">
         <div class="col-lg-12">
            <div class="card-header">
              <div class="card-title">
                <h3 class="card-label float-left ">Presupuesto de combustible</h3>
                  <%= link_to 'Regresar', fuel_budgets_path, class:"btn btn-default float-right" %>
                </div>
             </div>
         </div>
      </div>
    <div class="card-body">
      <p>
        <strong>Descripcion:</strong>
        <%= @fuel_budget.descripcion %>
      </p>

      <p>
        <strong>Precio litro:</strong>
        <%= @fuel_budget.precio_litro %>
      </p>
      <%= link_to 'Editar', edit_fuel_budget_path(@fuel_budget), class:"btn btn-default" %>
    </div>
  </div>
</div>